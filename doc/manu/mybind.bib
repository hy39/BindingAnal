@misc{Carrat2007,
abstract = {Influenza continues to have a major worldwide impact, resulting in considerable human suffering and economic burden. The regular recurrence of influenza epidemics is thought to be caused by antigenic drift, and a number of studies have shown that sufficient changes can accumulate in the virus to allow influenza to reinfect the same host. To address this, influenza vaccine content is reviewed annually to ensure protection is maintained, despite the emergence of drift variants; however, it is not always possible to capture every significant drift, partly due to the timing of the recommendations. Vaccine mismatch can impact on vaccine effectiveness, and has significant epidemiological and economical consequences, as was seen most apparently in the 1997-1998 influenza season. To meet the challenge of antigenic drift, vaccines that confer broad protection against heterovariant strains are needed against seasonal, epidemic and pandemic influenza. In addition to the use of vaccine adjuvants, emerging research areas include development of a universal vaccine and the use of vaccines that exploit mechanisms of cross-protective immunity. © 2007 Elsevier Ltd. All rights reserved.},
author = {Carrat, F. and Flahault, A.},
booktitle = {Vaccine},
doi = {10.1016/j.vaccine.2007.07.027},
isbn = {0264-410X (Print)$\backslash$r0264-410X (Linking)},
issn = {0264410X},
keywords = {Antigenic drift,Influenza,Vaccination},
number = {39-40},
pages = {6852--6862},
pmid = {17719149},
title = {{Influenza vaccine: The challenge of antigenic drift}},
volume = {25},
year = {2007}
}
@article{Das2011,
abstract = {Here, we address the question of why the influenza A virus hemagglutinin (HA) does not escape immunity by hyperglycosylation. Uniquely among dozens of monoclonal antibodies specific for A/Puerto Rico/8/34, escape from H28-A2 neutralization requires substitutions introducing N-linked glycosylation at residue 131 or 144 in the globular domain. This escape decreases viral binding to cellular receptors, which must be compensated for by additional substitutions in HA or neuraminidase that enable viral replication. Sequence analysis of circulating H1 influenza viruses confirms the in vivo relevance of our findings: natural occurrence of glycosylation at residue 131 is always accompanied by a compensatory mutation known to increase HA receptor avidity. In vaccinated mice challenged with WT vs. H28-A2 escape mutants, the selective advantage conferred by glycan-mediated global reduction in antigenicity is trumped by the costs of diminished receptor avidity. These findings show that, although N-linked glycosylation can broadly diminish HA antigenicity, fitness costs restrict its deployment in immune evasion.},
author = {Das, Suman R and Hensley, Scott E and David, Alexandre and Schmidt, Loren and Gibbs, James S and Puigb\`{o}, Pere and Ince, William L and Bennink, Jack R and Yewdell, Jonathan W},
doi = {10.1073/pnas.1108754108},
file = {:C$\backslash$:/Users/hy1613/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Das et al. - 2011 - Fitness costs limit influenza A virus hemagglutinin glycosylation as an immune evasion strategy.pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
month = dec,
number = {51},
pages = {E1417--22},
pmid = {22106257},
title = {{Fitness costs limit influenza A virus hemagglutinin glycosylation as an immune evasion strategy.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3251056\&tool=pmcentrez\&rendertype=abstract},
volume = {108},
year = {2011}
