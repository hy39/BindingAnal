pmid = {23824816},
title = {{Single hemagglutinin mutations that alter both antigenicity and receptor binding avidity influence influenza virus antigenic clustering.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3754131\&tool=pmcentrez\&rendertype=abstract},
volume = {87},
