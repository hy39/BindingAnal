journal = {Journal of virology},
keywords = {Animals,Antigenic Variation,Antigens, Viral,Antigens, Viral: genetics,Antigens, Viral: metabolism,Chick Embryo,Hemagglutinin Glycoproteins, Influenza Virus,Hemagglutinin Glycoproteins, Influenza Virus: gene,Hemagglutinin Glycoproteins, Influenza Virus: meta,Mutant Proteins,Mutant Proteins: genetics,Mutant Proteins: metabolism,Mutation, Missense,N-Acetylneuraminic Acid,N-Acetylneuraminic Acid: metabolism,Neuraminidase,Neuraminidase: genetics,Neuraminidase: metabolism,Orthomyxoviridae,Orthomyxoviridae: genetics,Orthomyxoviridae: immunology,Protein Binding,Receptors, Virus,Receptors, Virus: metabolism,Reverse Genetics,Suppression, Genetic,Viral Proteins,Viral Proteins: genetics,Viral Proteins: metabolism},
number = {20},
